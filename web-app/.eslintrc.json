{
  "extends": [
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["jest"],
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "prefer-const": "error",
    "no-var": "error",
    "no-undef": "off",
    "no-unused-vars": "off",
    "react-hooks/exhaustive-deps": "warn"
  }
}